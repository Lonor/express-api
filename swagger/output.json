{
  "openapi": "3.0.0",
  "info": {
    "title": "Express API - Template",
    "description": "[Source Code](https://github.com/lonor/express-api)",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:8080/api",
      "description": "Local"
    },
    {
      "url": "{URL}/api",
      "description": "Custom Server URL",
      "variables": {
        "URL": {
          "default": "https://node.lawrenceli.me"
        }
      }
    }
  ],
  "paths": {
    "/sse": {
      "get": {
        "description": "",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/hello": {
      "post": {
        "description": "Send Hello...",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "example": "any"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/bye/{name}": {
      "get": {
        "description": "Send bye...",
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/slow": {
      "get": {
        "description": "",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  }
}
