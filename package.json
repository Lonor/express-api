{
  "name": "node-express-example",
  "version": "0.0.1",
  "exports": "./app.js",
  "type": "module",
  "engines": {
    "node": ">=14.16"
  },
  "author": "Lawrence Li",
  "homepage": "https://github.com/Lonor/node-express-example",
  "license": "MIT",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "dev": "pnpm run swagger; pnpm start",
    "start": "node server.js",
    "test": "mocha --timeout 10000 --exit --reporter mocha-junit-reporter",
    "test:coverage": "c8 --reporter=lcov -r html -r text npm test",
    "test:grep": "pnpm test -- -g",
    "swagger": "node ./swagger/autogen.js && pnpm run format",
    "format": "prettier --write . --ignore-unknown",
    "prepare": "husky install"
  },
  "dependencies": {
    "dotenv": "16.0.2",
    "express": "4.18.1",
    "express-winston": "4.2.0",
    "node-fetch": "3.2.10",
    "swagger-ui-express": "4.5.0",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "c8": "7.12.0",
    "chai": "4.3.6",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "mocha": "10.0.0",
    "mocha-junit-reporter": "2.0.2",
    "prettier": "2.7.1",
    "supertest": "6.2.4",
    "swagger-autogen": "2.21.7"
  }
}
