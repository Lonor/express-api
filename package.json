{
  "name": "express-example",
  "version": "0.0.1",
  "exports": "./app.js",
  "type": "module",
  "engines": {
    "node": ">=14.16"
  },
  "author": "Lawrence",
  "homepage": "https://github.com/Lonor/express-api",
  "license": "MIT",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "start": "npm run swagger; node server.js",
    "test": "mocha --timeout 10000 --exit",
    "test:grep": "mocha --timeout 10000 --exit -g",
    "swagger": "node ./swagger/autogen.js && prettier --write ./swagger/output.json",
    "format": "prettier --write . --ignore-unknown"
  },
  "dependencies": {
    "dotenv": "16.0.0",
    "express": "4.18.0",
    "morgan": "1.10.0",
    "node-fetch": "3.2.3",
    "swagger-ui-express": "4.3.0"
  },
  "devDependencies": {
    "chai": "4.3.6",
    "husky": "7.0.4",
    "lint-staged": "12.4.1",
    "mocha": "9.2.2",
    "prettier": "2.6.2",
    "supertest": "6.2.3",
    "swagger-autogen": "2.21.1"
  }
}
